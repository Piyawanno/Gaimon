<div>
	<div class="abstract_container" rel="container">
		<div class="abstract_menu_container hidden" rel="menu">
			
		</div>
		<div class="abstract_data_container" rel="dataContainer">
			<div class="flex space-between">
				<div class="flex gap-20px">
					<div class="data_header" rel="title" localize>{{name}}</div>
				</div>
			</div>
			<div style="display:flex;gap:10px;">
				<div size="1x1" draggable="true" ondragstart="main.page.dashboard.drag(event)" style="width:100px;height:100px;background:#000;"></div>
				<div size="2x1" draggable="true" ondragstart="main.page.dashboard.drag(event)" style="width:200px;height:100px;background:#000;"></div>
				<div size="1x2" draggable="true" ondragstart="main.page.dashboard.drag(event)" style="width:100px;height:200px;background:#000;"></div>
				<div size="2x2" draggable="true" ondragstart="main.page.dashboard.drag(event)" style="width:200px;height:200px;background:#000;"></div>
			</div>
			<div rel="table"></div>
		</div>
	</div>
</div>