<div class="abstract_dialog">
	<div class="abstract_dialog_container abstract_dialog_form abstract_form" rel="form_container">
		<div class="titleContainer" rel="titleContainer">
			<div class="form_header" rel="title" localize>{{{title}}}</div>
			<div class="hidden" rel="switch">
				<div class="advanceSwitch button b2">
					<input type="checkbox" class="checkbox" rel="isAdvance">
					<div class="knobs">
						<span></span>
					</div>
					<div class="layer"></div>
				</div>
			</div>
			<div class="flex space-between gap-5px">
				<div class="flex flex-column-responsive gap-5px" style="height: 30px;" rel="operationContainer"></div>
			</div>
		</div>
		<div class="abstract_menu_container hidden" rel="menu">
			<div class="flex-wrap" rel="menuList"></div>
			<div rel="buttonList"></div>
		</div>
		<div class="abstract_form_input abstract_dialog_input" rel="form"></div>
		<div class="abstract_dialog_operation" rel="operation"></div>
	</div>
</div>